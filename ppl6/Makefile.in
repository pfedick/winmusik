###########################################################################
### Targets
###########################################################################


release: release/$(LIBNAME).a
	
debug: debug/$(LIBDEBUG).a
	
all:	debug/$(LIBDEBUG).a release/$(LIBNAME).a

debug/$(LIBDEBUG).a: debug/make $(DEBUG)
	ar -r debug/$(LIBDEBUG).a $(DEBUG)
	
release/$(LIBNAME).a: release/make $(RELEASE)
	ar -r release/$(LIBNAME).a $(RELEASE)
	
debug/make:
	-mkdir -p debug
	-touch debug/make

release/make:
	-mkdir -p release
	-touch release/make


clean:
	-rm -rf release
	-rm -rf debug

	 
