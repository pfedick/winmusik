incdir	= include

prefix	?= @prefix@
exec_prefix	= @exec_prefix@
TARGETLIB	?= @libdir@
TARGETBIN	?= @bindir@
TARGETINCLUDE	?= @includedir@
CC			= @CC@
CXX			= @CXX@
CFLAGS		=  -I${incdir} -DPPL6LIB @CFLAGS@ @DEFS@  @PTHREAD_CFLAGS@ 	@ZLIB_CFLAGS@ @BZ2_CFLAGS@ @PCRE_CFLAGS@ @OPENSSL_INCLUDES@ @ICONV_CFLAGS@ 	@LIBCDIO_CFLAGS@ @LIBCURL_CPPFLAGS@	
	
