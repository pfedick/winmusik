End Sub

Private Sub win_Click(Index As Integer)
If copyrightvisible = True Then Unload CopyrightForm
If Index <= 8 Then
  If Index = traeger And eingabevisible = True Then
    Unload eingabe
  Else
    If eingabevisible = True Then Unload eingabe
    traeger = Index
    eingabe.Show
  End If
ElseIf Index = 30 Then
  If quicksearchvisible = False Then
    quicksearch.Show
  Else
    Unload quicksearch
  End If
ElseIf Index = 31 Then
  If cdplayervisible = False Then
    'cdplayer.Show
    
  Else
    'Unload cdplayer
    
  End If
ElseIf Index = 32 Then
   leiste1.MousePointer = 11
   If eingabevisible = True Then Unload eingabe
   'start2.Show
   property1.Show
   'Unload leiste1
   leiste1.MousePointer = 0
ElseIf Index = 33 Then
    If sqlexportvisible = True Then
        Unload sqlexport
    Else
        sqlexport.Show
    End If
End If
End Sub

Private Sub win_MouseMove(Index As Integer, Button As Integer, Shift As Integer, X As Single, Y As Single)
On Error Resume Next
If (lastmausover <> Index) Then
  win(lastmausover).Picture = img(lastmausover).Picture
  lastmausover = Index
End If
win(Index).Picture = img(Index + 100).Picture

End Sub


